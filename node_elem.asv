clear all
Num = 4; % Number of intervals
NO = Num+1; % Number of points
h = 1/Num;
node = sparse ((Num+1)^2,2); 
y = 0:h:1;
[X,Y] = meshgrid(1,y);
Y = repmat(Y,(Num+1),1);
x = 0:h:1;
z = 0:h:1;
[Z] = meshgrid(x,z);
node (:,1) = Z(:);
node (:,2)=Y;
node = full(node); 
elem = sparse (2*Num^2,3);
   

